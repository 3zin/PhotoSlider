# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'PhotoSlider' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # UI
  pod 'SnapKit'
  pod 'Kingfisher'

  # Networking
  pod 'Moya/RxSwift'

  # Reactive frameworks
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RxOptional'

  # Syntactic sugar
  pod 'Then'

  target 'PhotoSliderTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'PhotoSliderUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
